{
  "name": "@omnious/ui",
  "version": "0.0.9-rc.22",
  "description": "React components for omnious.",
  "homepage": "https://omnious-ui.netlify.com",
  "author": "Omnious Dev Team <dev@omnious.com> (https://www.omnious.com)",
  "maintainers": [
    {
      "name": "Euihyun Yang",
      "email": "ykd1217@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": "https://github.com/omnious-dev/omnious-web-utils/tree/master/packages/ui",
  "keywords": [
    "omnious",
    "ui",
    "react",
    "components"
  ],
  "engines": {
    "node": ">= 8.10.0"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf .out dist",
    "build": "run-s build:**",
    "build:storybook": "build-storybook -c .storybook -o .out",
    "build:ui": "tsc",
    "lint": "run-s lint:**",
    "lint:js": "eslint .",
    "lint:ts": "tslint -p . -t stylish",
    "lint:css": "stylelint . --syntax scss",
    "prepublish": "yarn build",
    "prettier": "prettier --write '**/*.{js,ts,tsx}'",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "shortid": "^2.2.8"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-select": "^1.2.1",
    "styled-components": "^3.3.3"
  }
